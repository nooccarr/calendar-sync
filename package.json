{
  "name": "calendar-sync",
  "version": "1.0.0",
  "description": "Synchronize Calendar Appointments between Acuity Scheduler and Open Dental",
  "main": "server.js",
  "author": "",
  "license": "ISC",
  "scripts": {
    "dev": "concurrently --kill-others \"npm:tunnel\" \"npm:nodemon\"",
    "k6": "k6 run --vus 50 --duration 30s k8script.js",
    "nodemon": "nodemon server.js",
    "start": "pm2 start ecosystem.config.js --env production",
    "tunnel": "ngrok start calendar-sync"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nooccarr/calendar-sync.git"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^2.0.22"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "mongoose": "^7.3.1",
    "uuid": "^9.0.0"
  }
}